{
  "name": "elm-stand-up",
  "version": "1.0.0",
  "description": "A daily stand-up meeting tool written in Elm.",
  "main": "index.js",
  "scripts": {
    "build": "elm make src/Main.elm --output=elm.js --debug",
    "dev": "npm run watch -s && npm run start",
    "start": "elm reactor --port=8083",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "chokidar \"**/*.elm\" -c \"npm run build\" --initial"
  },
  "keywords": [
    "elm",
    "stand-up",
    "agile",
    "scrum"
  ],
  "author": "Jared M. Smith <absynce@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "elm": "^0.19.0-bugfix2"
  },
  "dependencies": {
    "elm-format": "^0.8.1"
  }
}
